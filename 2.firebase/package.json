{
  "name": "2.firebase",
  "version": "1.0.0",
  "description": "react, webpack, firebase",
  "main": "./app/index.js",
  "scripts": {
    "dev": "set NODE_ENV=development& webpack-dev-server --hot --inline --devtool eval --progress --colors",
    "dev:prod": "set NODE_ENV=production& webpack-dev-server --devtool eval --progress --colors --content-base build",
    "reinstall": "rimraf node_modules && yarn install",
    "build": "rimraf dist & set NODE_ENV=production& webpack",
    "deploy": "yarn build & firebase deploy"
  },
  "author": "oahehc",
  "license": "MIT",
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "lodash": "^4.17.5",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.0",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  }
}
